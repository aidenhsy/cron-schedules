{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "baseUrl": ".", // required for 'generated/*'
    "paths": {
      "generated/*": ["generated/*"] // match tsconfig.json
    }
  },
  "include": ["src", "generated"],
  "exclude": ["node_modules", "test", "dist", "**/*spec.ts"]
}
